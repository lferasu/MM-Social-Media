<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
    <welcome-file>edit_profile.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>HelloServelt</servlet-name>
    <servlet-class>com.mum.msm.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServelt</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <filter>
    <filter-name>loginFilter</filter-name>
    <filter-class>com.mum.msm.controller.LoginFilterServlet</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loginFilter</filter-name>
    <url-pattern>/login</url-pattern>
  </filter-mapping>

</web-app>
